from collections import Counter
from datetime import datetime

print(a:=datetime.utcnow())
input1 = Counter([3,1,3,2,3,1,2,2,3])
print(input1)
print(datetime.utcnow()-a)

#counter needs to be checked.

a = datetime.utcnow()
dict_output = dict()
for i in input1:
    if not i in dict_output.keys():
        dict_output[i]=1
    else:
        dict_output[i]+=1


print(dict_output)
print(datetime.utcnow()-a)

l1 = [5,1,3,2]


# sort without using any sort functions
for j in range(len(l1)):
    for i in range(len(l1)-1):
        if l1[i+1]<l1[i]:
            l1[i],l1[i+1]=l1[i+1],l1[i]

print(l1)


input1 = [1,2,3,4]
input2 = [4,3,2,1]

print(dict(zip(input1,input2)))


# output = {1:4,2:3,3:2,4:1}
